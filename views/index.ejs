<% include ./partials/header.ejs%>
<h1>This is the index</h1>
<% blogs.forEach(function (blog) { %>
    <div class="blog">
        <h2> <%= blog.title%></h2>
        <img src="<%= blog.image%>">
        <p><%= blog.body%></p>
        <a href="/blogs/<%=blog.id %>">Read More</a>
        <span><%= blog.created%></span>
    </div>
<%})%>
<% include ./partials/footer.ejs%>